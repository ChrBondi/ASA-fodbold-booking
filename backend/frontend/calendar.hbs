<div>
  <div class="month">
    <ul>
      <li class="prev">&#10094;</li>
      <li class="next">&#10095;</li>
      <li>August<br><span style="font-size:18px">2017</span></li>
    </ul>
  </div>

  <ul class="weekdays">
    <li>Mandag</li>
    <li>Tirsdag</li>
    <li>Onsdag</li>
    <li>Torsdag</li>
    <li>Fredag</li>
    <li>Lørdag</li>
    <li>Søndag</li>
  </ul>

  <ul class="days">
    <li>1</li>
    <li>2</li>
    <li>3</li>
    <li>4</li>
    <li>5</li>
    <li>6</li>
    <li>7</li>
    <li>8</li>
    <li>9</li>
    <li><span class="active">10</span></li>
    <li>11</li>
    ...etc
  </ul>
</div>

<div style="display:flex;">
  {{#each fields}}
  <ul>
    <li><b>{{@key}}</b></li>
    {{#each this}}
    <li>
        {{formatBooking this}}
    </li>
    {{/each}}
  </ul>
  {{/each}}
</div>
